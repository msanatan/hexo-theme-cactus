<% if (index) { %>
    <% if (post['link']) { %>
        <a class="<%= class_name %>" href="<%- post['link'] %>" target="_blank" itemprop="url"><%= post['title'] %></a>
    <% } %>
<% } else { %>
    <h1 class="<%= class_name %>" itemprop="name headline">
        <%= post['title'] %>
    </h1>
<% } %>

